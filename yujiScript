local Players = game:GetService("Players")

local player = Players.LocalPlayer

local playerGui = player:WaitForChild("PlayerGui")


local function findGuiAndSetText()

    local screenGui = playerGui:FindFirstChild("ScreenGui")

    if screenGui then

        local magicHealthFrame = screenGui:FindFirstChild("MagicHealth")

        if magicHealthFrame then

            local textLabel = magicHealthFrame:FindFirstChild("TextLabel")

            if textLabel then

                textLabel.Text = "16 Finger Sukuna"

            end

        end

    end

end


playerGui.DescendantAdded:Connect(findGuiAndSetText)

findGuiAndSetText()

local animationId = 12273188754

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local function onAnimationPlayed(animationTrack)
    if animationTrack.Animation.AnimationId == "rbxassetid://" .. animationId then
        -- Stop all currently playing animations
        for _, animTrack in pairs(humanoid:GetPlayingAnimationTracks()) do
            animTrack:Stop()
        end

        local AnimAnim = Instance.new("Animation")
        AnimAnim.AnimationId = "rbxassetid://13560306510"

        local Anim = humanoid:LoadAnimation(AnimAnim)
        Anim.TimePosition = 0

        Anim:Play()
        Anim:AdjustSpeed(2.78)

        -- Call the function to destroy WaterPalm parts immediately after animation starts
        local function destroyWaterPalmParts()
            local rightArm = character:FindFirstChild("Right Arm")
            local leftArm = character:FindFirstChild("Left Arm")

            if rightArm then
                local waterPalmRight = rightArm:FindFirstChild("WaterPalm")
                if waterPalmRight then
                    waterPalmRight:Destroy()
                end
            end

            if leftArm then
                local waterPalmLeft = leftArm:FindFirstChild("WaterPalm")
                if waterPalmLeft then
                    waterPalmLeft:Destroy()
                end
            end
        end

        -- Call the function to destroy WaterPalm parts when the animation starts
        destroyWaterPalmParts()

        -- Optionally continue running loadstring if needed
        local function runLoadString()
            while Anim.IsPlaying do -- Check if the animation is still playing
                
            local duration = 1
            local interval = 0.1
                
            local final1 = game.ReplicatedStorage.Resources.FiveSeasonsFX.CharFX.ArmBurst.a:Clone()
            final1.Parent = game.Players.LocalPlayer.Character["Right Arm"]
            final1:FindFirstChild("2"):Destroy()
            for _, child in ipairs(final1:GetChildren()) do
                if child:IsA("ParticleEmitter") then -- Check if the child is a ParticleEmitter
                        child:Emit(1) -- Emit 20 particles
                end
            end

            local duration = 1
            local interval = 0.1
                
            local final2 = game.ReplicatedStorage.Resources.FiveSeasonsFX.CharFX.ArmBurst.a:Clone()
            final2.Parent = game.Players.LocalPlayer.Character["Left Arm"]
            final2:FindFirstChild("2"):Destroy()
            for _, child in ipairs(final2:GetChildren()) do
                if child:IsA("ParticleEmitter") then -- Check if the child is a ParticleEmitter
                        child:Emit(1) -- Emit 20 particles
                end
            end
            
                wait() -- Wait a bit before running it again to avoid spam
            end
        end

        runLoadString() -- Call the function to start running loadstring in a loop
    end
end

humanoid.AnimationPlayed:Connect(onAnimationPlayed)

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rightArm = character:WaitForChild("Right Arm")
local leftArm = character:WaitForChild("Left Arm")

local function destroyWaterPalm()
    local rightWaterPalm = rightArm:FindFirstChild("WaterPalm")
    local leftWaterPalm = leftArm:FindFirstChild("WaterPalm")

    if rightWaterPalm then
        rightWaterPalm:Destroy()
    end
    if leftWaterPalm then
        leftWaterPalm:Destroy()
    end
end

task.spawn(function()
while true do
    -- This will continuously check for WaterPalm in both arms
    -- and destroy it if it exists
    if rightArm:FindFirstChild("WaterPalm") or leftArm:FindFirstChild("WaterPalm") then
        destroyWaterPalm()
    end

    wait(0.01) -- Small wait to prevent performance issues
end
end)

